{
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": [
    "prettier",
    "vue/essential"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 8
  },
  "rules": {
    "semi": ["error", "never"],
    "no-unused-expressions": "off",
    "indent": [2, 2]
  }
}
