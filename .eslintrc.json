{
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": [
    "airbnb-base",
    "prettier",
    "plugin:vue/essential"
  ],
  "plugins": [
    "prettier",
    "html"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 8
  },
  "rules": {
    "semi": ["error", "never"],
    "no-unused-expressions": "off",
    "indent": [2, 2],
    "import/no-extraneous-dependencies": 0
  }
}
